#include <stdio.h>
#include <math.h>

int main() {
    int num;
    float root;

    printf("Enter a number: ");
    scanf("%d", &num);

    root = sqrt(num);

    if (root == (int)root) {
        printf("%d is a perfect square\n", num);
    } else {
        printf("%d is not a perfect square\n", num);
    }

    return 0;
}
